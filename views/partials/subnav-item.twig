 <li class="subnav-item -site">
    <a class="subnav-item-link" href="{{child.get_link}}">
        <div class="subnav-item-wrap">
            {% set term = TimberTerm(child.menu_object.object_id) %}
            {% set img = TimberImage(term.featured_image) %}
            <div class="subnav-item-part -first">
                <img data-src="{{img.src|resize(200)}}" src="{{placeholder}}" class="tease-img lazyload" alt="{{child.name}} Thumbnail">
            </div>
            <div class="subnav-item-part">
                <h4 class="subnav-item-title">{{child.title}}</h4>
                <p class="subnav-item-desc">{{term.description}}</p>
            </div>
        </div>
    </a>
</li>