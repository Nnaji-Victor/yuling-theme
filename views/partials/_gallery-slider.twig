<div class="gallery js-flickity"
   data-flickity-options='{ "lazyLoad": 3, "initialIndex": 1, "wrapAround": true, "pageDots": false, "freeScroll": true }'>

	{% for image in gallery %}
	  <img class="gallery-cell-image" data-flickity-lazyload="{{image.url|resize(1200)}}" alt="tulip" /> 	
	{% endfor %}
</div>
