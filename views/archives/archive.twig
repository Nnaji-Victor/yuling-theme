{% extends 'base.twig' %}

{% block before_content %}
	{% include 'headers/_header-archive.twig' with { class : 'header--archive' } %}
{% endblock %}

{% block content %}
	{% for post in posts %}
		{% set post = TimberPost(post) %}
		{% include 'teases/_tease-' ~ post.post_type ~ '.twig' %}
	{% endfor %}
{% endblock %}